{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "schema",
  "properties": {
    "schemaVersion": {
      "type": "string",
      "title": "schemaVersion",
      "description": "The version number of the schema in use for this instance",
      "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
      "examples": [
        "1.2.1", "6.0.0"
      ]
    }
  },
  "additionalProperties": false
}
