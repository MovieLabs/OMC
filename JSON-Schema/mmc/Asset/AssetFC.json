{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"title": "Asset Functional Characteristics",
	"description": "A set of Functional Characteristics for Assets",
	"required": [
		"identifier",
		"entityType"
	],
	"properties": {
		"entityInfo": {
			"title": "Entity Information",
			"$ref": "../Model/definitions.json#/properties/entityInfo"
		},
		"entityType": {
			"type": "string",
			"title": "Entity Type",
			"const": "AssetFC"
		},
		"identifier": {
			"$ref": "../Utility/identifier.json"
		},
		"characteristics": {
			"anyOf": [
				{
					"type": "array",
					"additionalItems": true,
					"items": {
						"type": "object",
						"properties": {
							"functionalType": {
								"type": "string",
								"title": "Functional Type"
							},
							"functionalName": {
								"type": "string",
								"title": "Name"
							},
							"description": {
								"type": "string",
								"title": "Description"
							}
						},
						"additionalProperties": true
					}
				}
			]
		}
	},
	"$defs": {
	}
}