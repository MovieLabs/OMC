{	"$schema": "http://json-schema.org/draft-07/schema#",	"type": "object",	"title": "Asset Structural Characteristics",	"description": "A set of Stuctural Characteristics for Assets",	"properties": {		"entityInfo": {			"title": "Entity Information",			"$ref": "../Model/definitions.json#/properties/entityInfo"		},		"entityType": {			"type": "string",			"title": "Entity Type",			"const": "AssetSC"		},		"identifier": {"$ref":"../Utility/identifier.json"},		"characteristics": {			"oneOf": [				{					"title": "Analog Moving-Image",					"type": "object",					"additionalProperties": true,					"properties": {						"structuralType": {							"type": "string",							"const": "movingImage.analog"						},						"dimensions": {"$ref":"../Utility/unitOfMeasurement.json#/properties/dimensions"},						"color": {"$ref":"#/$defs/color"},						"sourceReference": {"$ref":"#/$defs/sourceReference"},						"customData": {"$ref":"../Model/definitions.json#/properties/customData"}					}				},				{					"title": "Digital Moving-Image",					"type": "object",					"additionalProperties": false,					"properties": {						"structuralType": {							"type": "string",							"const": "movingImage.digital"						},						"dimensions": {"$ref":"../Utility/unitOfMeasurement.json#/properties/dimensions"},						"colorSpace": {"$ref":"#/$defs/colorSpace"},						"fileExtension": {"$ref":"#/$defs/fileExtension"},						"fileEncoding": {"$ref":"#/$defs/fileEncoding"},						"sourceReference": {"$ref":"#/$defs/sourceReference"},						"customData": {"$ref":"../Model/definitions.json#/properties/customData"}					}				},				{					"title": "Analog Image",					"description": "A set of properties describing the Structural Characteristics of static images",					"type": "object",					"additionalProperties": false,					"properties": {						"structuralType": {							"type": "string",							"const": "image.analog"						},						"dimensions": {"$ref":"../Utility/unitOfMeasurement.json#/properties/dimensions"},						"color": {"$ref":"#/$defs/color"},						"format": {							"title": "Image format",							"description": "The format the image is available in",							"type": "string",							"enum": ["paper","negative","slide","glass plate"]						},						"sourceReference": {"$ref":"#/$defs/sourceReference"},						"customData": {"$ref":"../Model/definitions.json#/properties/customData"}					}				},				{					"title": "Digital Image",					"description": "A set of properties describing the Structural Characteristics of static images",					"type": "object",					"additionalProperties": false,					"properties": {						"structuralType": {							"type": "string",							"const": "image.digital"						},						"dimensions": {"$ref":"../Utility/unitOfMeasurement.json#/properties/dimensions"},						"colorSpace": {"$ref":"#/$defs/colorSpace"},						"channels": {							"type": "string",							"title": "Channels"						},						"fileExtension": {"$ref":"#/$defs/fileExtension"},						"fileEncoding": {"$ref":"#/$defs/fileEncoding"},						"sourceReference": {"$ref":"#/$defs/sourceReference"},						"customData": {"$ref":"../Model/definitions.json#/properties/customData"}					}				},				{					"title": "Analog Stuctured Document",					"type": "object",					"additionalProperties": false,					"properties": {						"structuralType": {							"type": "string",							"const": "structuredDocument.analog"						},						"sourceReference": {"$ref":"#/$defs/sourceReference"},						"customData": {"$ref":"../Model/definitions.json#/properties/customData"}					}				},				{					"title": "Digital Stuctured Document",					"type": "object",					"additionalProperties": false,					"properties": {						"structuralType": {							"type": "string",							"const": "structuredDocument.digital"						},						"sourceReference": {"$ref":"#/$defs/sourceReference"},						"fileExtension": {"$ref":"#/$defs/fileExtension"},						"fileEncoding": {"$ref":"#/$defs/fileEncoding"},						"customData": {"$ref":"../Model/definitions.json#/properties/customData"}					}				}			]		}	},	"$defs": {		"sourceReference": {			"title": "Source Reference",			"description": "A set of terms that describe the source of the image, such as url, the name of publication, stock library etc.",			"type": "array",			"items": {				"type": "string",				"examples": ["Library of congress","wikipedia.com","Getty Images"]			}		},		"color": {			"title": "Color",			"anyOf": [				{"type":"string"},				{					"enum": ["color","monochrome","colorized"]				}			]		},		"colorSpace": {			"type": "string",			"title": "Color Space",			"description": "Image color-space",			"examples": ["sRGB","Adobe RGB"]		},		"fileExtension": {			"type": "string",			"title": "File Extension",			"description": "The file extension for the asset, often a 3 or 4 letter code that typically indicates the encoding format",			"examples": ["jpg","png","pdf","mp4"]		},		"fileEncoding": {			"type": "string",			"title": "File Encoding",			"description": "The encoding used for asset",			"examples": ["jpeg2000","h.263"]		}	}}