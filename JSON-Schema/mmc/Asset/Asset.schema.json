{	"$schema": "http://json-schema.org/draft-07/schema#",	"$id": "Asset#Asset",	"title": "Asset",	"description": "A single referencable object or file used in the Creative Work",	"type": "object",	"properties": {		"assetType": {			"title": "assetType",			"description": "The type of asset",			"type": "string"		},		"Identifier": {			"$ref": "../Utility/Identifier.schema.json"		},		"StructuralCharacteristics": {			"title": "Structural Characteristics",			"oneOf": [				{					"anyOf": [						{ "$ref": "StructuralCharacteristics/Digital-Image.schema.json" },						{ "$ref": "StructuralCharacteristics/Analog-Image.schema.json" },						{ "$ref": "StructuralCharacteristics/Digital-MovingImage.schema.json" },						{ "$ref": "StructuralCharacteristics/Analog-MovingImage.schema.json" }					]				},				{					"type": "array",					"items": [						{							"$ref": "StructuralCharacteristics/Digital-Image.schema.json",							"$ref": "StructuralCharacteristics/Analog-Image.schema.json",							"$ref": "StructuralCharacteristics/Digital-MovingImage.schema.json",							"$ref": "StructuralCharacteristics/Analog-MovingImage.schema.json"						}					]				}			]		},		"FunctionalCharacteristics": {			"title": "Functional",			"type": "object",			"properties": {				"Name": {					"$ref": "../Utility/Name.schema.json"				}			}		}	},	"required": [ "assetType", "Identifier", "StructuralCharacteristics", "FunctionalCharacteristics" ],	"additionalProperties": false}