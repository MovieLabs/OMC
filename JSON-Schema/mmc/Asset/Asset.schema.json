{	"$schema": "http://json-schema.org/draft-07/schema#",	"$id": "#Asset",	"title": "Asset",	"description": "A single referenceable object or file used in the Creative Work",	"type": "object",	"properties": {		"Identifier": {			"$ref": "../Utility/Identifier.schema.json"		},		"assetType": {			"title": "assetType",			"description": "The type of asset",			"type": "string"		},		"Name": {			"$ref": "../Utility/Name.schema.json#"		},		"description": {			"title": "Description",			"type" : "string",			"description": "A brief description of the asset"		},		"StructuralCharacteristics": {			"title": "Structural Characteristics",			"oneOf": [				{"$ref": "#/definitions/StructuralClass"},				{					"type": "array",					"items": [						{"$ref": "#/definitions/StructuralClass"}					]				}			]		},		"FunctionalCharacteristics": {			"title": "Functional",			"type": "object",			"properties": {				"Name": {					"$ref": "../Utility/Name.schema.json"				}			}		},		"Relationship": {			"$ref": "../Relationship/Relationship.schema.json"		}	},	"required": [		"assetType",		"Identifier",		"StructuralCharacteristics",		"FunctionalCharacteristics"	],	"additionalProperties": false,	"definitions": {		"StructuralClass": {			"anyOf": [						{							"$ref": "./StructuralCharacteristics/Digital-Image.schema.json"						},						{							"$ref": "./StructuralCharacteristics/Analog-Image.schema.json"						},						{							"$ref": "./StructuralCharacteristics/Digital-MovingImage.schema.json"						},						{							"$ref": "./StructuralCharacteristics/Analog-MovingImage.schema.json"						}			]		}	}}