{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "#StructuralDigitalImage",
	"description": "Structural Characteristics of an Digital Image",
	"type": "object",
	"properties": {
		"structuralType": {
			"title": "structuralType",
			"type": "string",
			"pattern": "Digital-Image"
		},
		"assetHash": {
			"title": "assetHash",
			"type": [
				"string",
				"null"
			]
		},
		"attributes": {
			"title": "attributes",
			"type": "object",
			"properties": {
				"imageWidth": {
					"type": "integer"
				},
				"imageHeight": {
					"type": "integer"
				}
			}
		}
	}
}
