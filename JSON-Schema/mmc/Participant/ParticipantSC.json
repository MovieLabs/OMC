{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"title": "Participant Structural Characteristics",
	"description": "A set of Structural Characteristics for Participants",
	"required": [
		"identifier",
		"entityType"
	],
	"properties": {
		"entityInfo": {
			"title": "Entity Information",
			"$ref": "../Model/definitions.json#/properties/entityInfo"
		},
		"entityType": {
			"type": "string",
			"title": "Entity Type",
			"const": "ParticipantSC"
		},
		"identifier": {
			"$ref": "../Utility/identifier.json"
		},
		"characteristics": {
			"oneOf": [
				{
					"title": "Organization",
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"structuralType": {
							"type": "string",
							"const": "organization"
						},
						"organizationName": {
							"type": "string",
							"title": "Organization Name"
						},
						"Location": {
							"$ref": "../Utility/Location.json"
						},
						"contact": {
							"$ref": "../Utility/contact.json"
						}
					}
				},
				{
					"type": "object",
					"title": "Department",
					"additionalProperties": false,
					"properties": {
						"structuralType": {
							"type": "string",
							"const": "department"
						},
						"departmentName": {
							"type": "string",
							"title": "Department Name"
						},
						"Location": {
							"$ref": "../Utility/Location.json"
						},
						"contact": {
							"$ref": "../Utility/contact.json"
						}
					}
				},
				{
					"type": "object",
					"title": "Person",
					"additionalProperties": false,
					"properties": {
						"structuralType": {
							"type": "string",
							"const": "person"
						},
						"personName": {
							"$ref": "../Utility/completeName.json"
						},
						"Location": {
							"$ref": "../Utility/Location.json"
						},
						"contact": {
							"$ref": "../Utility/contact.json"
						}
					}
				},
				{
					"type": "object",
					"title": "Service",
					"additionalProperties": false,
					"properties": {
						"structuralType": {
							"type": "string",
							"const": "servcie"
						},
						"serviceName": {
							"type": "string",
							"title": "Service Name"
						},
						"Location": {
							"$ref": "../Utility/Location.json"
						},
						"contact": {
							"$ref": "../Utility/contact.json"
						}
					}
				}
			]
		}
	}
}