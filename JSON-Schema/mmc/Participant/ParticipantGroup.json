{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"title": "Participant Group",
	"description": "A Participant composed of other Participants, where the assemblage is treated as a single unit",
	"required": [
		"identifier",
		"entityType"
	],
	"properties": {
		"entityInfo": {
			"title": "Entity Information",
			"$ref": "../Model/definitions.json#/properties/entityInfo"
		},
		"entityType": {
			"type": "string",
			"title": "Entity Type",
			"const": "ParticipantGroup"
		},
		"identifier": {
			"$ref": "../Utility/identifier.json"
		},
		"name": {
			"type": [
				"string",
				"null"
			],
			"title": "Name"
		},
		"description": {
			"type": [
				"string",
				"null"
			],
			"title": "Description"
		},
		"isOrdered": {
			"type": "boolean",
			"title": "Is ordered",
			"description": "Set to true of the Participant array is in a prescribed order"
		},
		"customData": {
			"title": "Custom Data",
			"$ref": "../Model/definitions.json#/properties/customData"
		},
		"Participant": {
			"type": "array",
			"description": "The group of Paraticipants that jointly comprise this Participant",
			"items": {
				"$ref": "./Participant.json"
			}
		},
		"StructuralCharacteristics": {
			"title": "Participant Group StructuralCharacteristics",
			"$ref": "./ParticipantSC.json"
		},
		"FunctionalCharacteristics": {
			"title": "Participant Group Functional Characteristics",
			"$ref": "./ParticipantFC.json"
		}
	}
}