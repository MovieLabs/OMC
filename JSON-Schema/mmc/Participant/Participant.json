{	"$schema": "http://json-schema.org/draft-07/schema#",	"title": "Participant",	"description": "The entities (people, organizations, and services) that are responsible for the production of the Creative Work.",	"type": "object",	"properties": {		"entityType": {			"title": "Entity Type",			"type": "string",			"const": "Participant"		},		"schemaVersion": {			"$ref": "../Model/definitions.json#/properties/schemaVersion"		},		"instance": {			"$ref": "../Model/definitions.json#/properties/instance"		},		"identifier": {			"$ref": "../Utility/identifier.json#/properties/identifier"		},		"description": {			"title": "Description",			"type": [				"string",				"null"			]		},		"participantGroup": {			"description": "The group of Participants that jointly comprise this Participant",			"type": "array",			"items": {				"$ref": "../Participant/Participant.json"			}		},		"roleGroup": {			"description": "A group of Roles this Participant can perform",			"type": "array",			"items": {				"$ref": "../Participant/Role.json"			}		},		"StructuralCharacteristics": {			"title": "Participant Structural Characteristics",			"$ref": "../Participant/ParticipantSC.json"		},		"FunctionalCharacteristics": {			"title": "Participant Functional Characteristics",			"$ref": "../Participant/ParticipantFC.json"		}	},	"required": [		"entityType",		"identifier"	],	"$defs": {	}}