{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"title": "NarrativeScene",
	"description": "Taken from the narrative itself and traditionally defined by creative intent and typically a unity of time, place, action, or theme.",
	"properties": {
		"schemaVersion": {
			"title": "schemaVersion",
			"$ref": "../Model/schema.json#/properties/schemaVersion"
		},
		"entityType": {
			"type": "string",
			"title": "Entity Type",
			"const": "NarrativeScene"
		},
		"instance": {
			"$ref": "../Model/instance.json"
		},
		"identifier": {
			"title": "identifier",
			"$ref": "../Utility/identifier.json"
		},
		"name": {
			"type": "string",
			"title": "Scene Name"
		},
		"description": {
			"type": "string",
			"title": "Description"
		},
		"slugline": {
			"type": "array",
			"title": "Slugline",
			"additionalItems": true,
			"items": {
				"type": "string"
			}
		},
		"Context": {
			"type": "object",
			"title": "Context",
			"properties": {
				"entityType": {
					"type": "string",
					"title": "Entity Type",
					"const": "Context"
				},
				"identifier": {
					"title": "identifier",
					"$ref": "../Utility/identifier.json"
				},
				"uses": {
					"type": "object",
					"title": "usesProp",
					"properties": {
						"NarrativeGreenery": {
							"type": "array",
							"items": {
								"$ref": "../MediaCreationContext/NarrativeGreenery.json"
							}
						},
						"SetDressing": {
							"type": "array",
							"items": {
								"$ref": "../MediaCreationContext/SetDressing.json"
							}
						},
						"NarrativeVehicle": {
							"type": "array",
							"items": {
								"$ref": "../MediaCreationContext/NarrativeVehicle.json"
							}
						}
					}
				},
				"usesProp": {
					"type": "object",
					"title": "usesProp",
					"properties": {
						"NarrativeProp": {
							"type": "array",
							"items": {
								"$ref": "../MediaCreationContext/NarrativeProp.json"
							}
						}
					}
				},
				"usesWardrobe": {
					"type": "object",
					"title": "usesWardrobe",
					"properties": {
						"NarrativeWardrobe": {
							"type": "array",
							"items": {
								"$ref": "../MediaCreationContext/NarrativeWardrobe.json"
							}
						}
					}
				},
				"features": {
					"type": "object",
					"title": "features",
					"properties": {
						"Character": {
							"type": "array",
							"items": {
								"$ref": "../MediaCreationContext/Character.json"
							}
						}
					}
				},
				"hasStoryBoard": {
					"type": "object",
					"title": "hasStoryBoard",
					"properties": {
						"Character": {
							"type": "array",
							"items": {
								"$ref": "../MediaCreationContext/StoryBoard.json"
							}
						}
					}
				}
			},
			"additionalProperties": true
		}
	},
	"additionalProperties": true
}