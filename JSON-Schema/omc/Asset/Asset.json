{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Asset",
  "description": "A physical or digital object or collection of objects specific to the creation of the Creative Work.",
  "required": [
    "identifier",
    "entityType"
  ],
  "properties": {
	"entityInfo": {
			"title": "Entity Information",
			"$ref": "../Model/definitions.json#/properties/entityInfo"
		},
		"entityType": {
		  "type": "string",
		  "title": "Entity Type",
		  "const": "Asset"
		},
		"identifier": {
		  "$ref": "../Utility/identifier.json"
		},
		"name": {
		  "type": [
			"string",
			"null"
		  ],
		  "title": "Name"
		},
		"description": {
		  "type": [
			"string",
			"null"
		  ],
		  "title": "Description"
		},
		"customData": {
			"title": "Custom Data",		
			"$ref": "../Model/definitions.json#/properties/customData"
		},
		"AssetGroup": {
			"title": "Asset Group",
			"$ref": "./AssetGroup.json"
		},
		"StructuralCharacteristics": {
		  "title": "StructuralCharacteristics",
		  "$ref": "./AssetSC.json"
		},
		"FunctionalCharacteristics": {
		  "title": "Functional Characteristics",
		  "$ref": "./AssetFC.json"
		}
	}
}
