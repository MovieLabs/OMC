{	"$schema": "http://json-schema.org/draft-07/schema#",	"type": "object",	"title": "Asset Structural Characteristics",	"description": "Describes the form of an Asset along with attributes specific to that Asset's form",	"properties": {		"entityInfo": {			"$ref": "../Model/definitions.json#/properties/entityInfo"		},		"entityType": {			"type": "string",			"title": "Entity Type",			"const": "AssetSC"		},		"identifier": {			"$ref": "../Utility/identifier.json"		},		"structuralType": {			"type": "string",			"title": "Structural Type",			"description": "A structured description of the an assets type",			"examples": [				"digital.image",				"analog.movingImage"			]		},		"structuralProperties": {			"$ref": "#/$defs/structuralProperties"		}	},	"$defs": {		"structuralProperties": {			"title": "Structural Properties",			"description": "A set of properties that describe attributes specific to the Asset",			"anyOf": [				{					"title": "AssetGroup",					"description": "An Asset that is comprised of other Assets or Asset Groups and treated as a single unit",					"type": "object",					"additionalProperties": true,					"properties": {						"isOrdered": {							"title": "Assets are ordered",							"type": "boolean",							"description": "True when the Assets in the group are ordered"						}					}				},				{					"title": "Analog Moving-Image",					"description": "",					"type": "object",					"additionalProperties": true,					"properties": {						"dimensions": {							"$ref": "../Utility/unitOfMeasurement.json#/properties/dimensions"						},						"color": {							"$ref": "#/$defs/color"						},						"sourceReference": {							"$ref": "#/$defs/sourceReference"						},						"customData": {							"$ref": "../Model/definitions.json#/properties/customData"						}					}				},				{					"title": "Digital Moving-Image",					"description": "",					"type": "object",					"additionalProperties": true,					"properties": {						"dimensions": {							"$ref": "../Utility/unitOfMeasurement.json#/properties/dimensions"						},						"colorSpace": {							"$ref": "#/$defs/colorSpace"						},						"fileExtension": {							"$ref": "#/$defs/fileExtension"						},						"fileEncoding": {							"$ref": "#/$defs/fileEncoding"						},						"sourceReference": {							"$ref": "#/$defs/sourceReference"						},						"format": {							"$ref": "#/$defs/digitalFormat"						},						"customData": {							"$ref": "../Model/definitions.json#/properties/customData"						}					}				},				{					"title": "Analog Image",					"description": "A set of properties describing the Structural Characteristics of static images",					"type": "object",					"additionalProperties": true,					"properties": {						"dimensions": {							"$ref": "../Utility/unitOfMeasurement.json#/properties/dimensions"						},						"color": {							"$ref": "#/$defs/color"						},						"format": {							"title": "Image format",							"description": "The format the image is available in",							"type": "string",							"enum": [								"paper",								"negative",								"slide",								"glass plate"							]						},						"sourceReference": {							"$ref": "#/$defs/sourceReference"						},						"customData": {							"$ref": "../Model/definitions.json#/properties/customData"						}					}				},				{					"title": "Digital Image",					"description": "A set of properties describing the Structural Characteristics of static images",					"type": "object",					"additionalProperties": true,					"properties": {						"dimensions": {							"$ref": "../Utility/unitOfMeasurement.json#/properties/dimensions"						},						"colorSpace": {							"$ref": "#/$defs/colorSpace"						},						"channels": {							"type": "string",							"title": "Channels"						},						"fileExtension": {							"$ref": "#/$defs/fileExtension"						},						"fileEncoding": {							"$ref": "#/$defs/fileEncoding"						},						"sourceReference": {							"$ref": "#/$defs/sourceReference"						},						"format": {							"$ref": "#/$defs/digitalFormat"						},						"customData": {							"$ref": "../Model/definitions.json#/properties/customData"						}					}				},				{					"title": "Analog Stuctured Document",					"description": "",					"type": "object",					"additionalProperties": true,					"properties": {						"sourceReference": {							"$ref": "#/$defs/sourceReference"						},						"customData": {							"$ref": "../Model/definitions.json#/properties/customData"						}					}				},				{					"title": "Digital Stuctured Document",					"description": "",					"type": "object",					"additionalProperties": true,					"properties": {						"sourceReference": {							"$ref": "#/$defs/sourceReference"						},						"fileExtension": {							"$ref": "#/$defs/fileExtension"						},						"fileEncoding": {							"$ref": "#/$defs/fileEncoding"						},						"format": {							"$ref": "#/$defs/digitalFormat"						},						"customData": {							"$ref": "../Model/definitions.json#/properties/customData"						}					}				}			]		},		"sourceReference": {			"title": "Source Reference",			"description": "A set of terms that describe the source of the image, such as url, the name of publication, stock library etc.",			"type": "array",			"items": {				"type": "string",				"examples": [					"Library of congress",					"wikipedia.com",					"Getty Images"				]			}		},		"color": {			"title": "Color",			"anyOf": [				{					"type": "string"				},				{					"enum": [						"color",						"monochrome",						"colorized"					]				}			]		},		"colorSpace": {			"type": "string",			"title": "Color Space",			"description": "Image color-space",			"examples": [				"sRGB",				"Adobe RGB"			]		},		"fileExtension": {			"type": "string",			"title": "File Extension",			"description": "The file extension for the asset, often a 3 or 4 letter code that typically indicates the encoding format",			"examples": [				"jpg",				"png",				"pdf",				"mp4"			]		},		"fileEncoding": {			"type": "string",			"title": "File Encoding",			"description": "The encoding used for asset",			"examples": [				"jpeg2000",				"h.263"			]		},		"digitalFormat": {			"type": "string",			"title": "Digital format",			"description": "The format of an asset, for digital files a MIME type is prefered",			"examples": [				"image/jpeg",				"text",				"audio/mpeg"			]		}	}}