{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "AssetGroup",
  "description": "Information about an Asset made out of other Assets, where the assemblage is treated as a single group",
  "required": [
    "identifier",
    "entityType"
  ],
  "properties": {
		"entityInfo": {
			"title": "Entity Information",
			"$ref": "../Model/definitions.json#/properties/entityInfo"
		},
		"entityType": {
		  "type": "string",
		  "title": "Entity Type",
		  "const": "AssetGroup"
		},
		"identifier": {
		  "$ref": "../Utility/identifier.json"
		},
		"name": {
		  "type": [
			"string",
			"null"
		  ],
		  "title": "Name"
		},
		"description": {
		  "type": [
			"string",
			"null"
		  ],
		  "title": "Description"
		},
		"isOrdered": {
			"type": "boolean",
			"title": "Is ordered",
			"description": "Set to true of the Asset array is in a prescribed order"
		},
		"customData": {
			"title": "Custom Data",		
			"$ref": "../Model/definitions.json#/properties/customData"
		},
		"Asset": {
		  "type": "array",
		  "description": "The group of Assets that jointly comprise this Asset",
		  "items": {
			"$ref": "./Asset.json"
		  }
		},
		"Asset Group StructuralCharacteristics": {
		  "title": "StructuralCharacteristics",
		  "$ref": "./AssetSC.json"
		},
		"Asset Group Characteristics": {
		  "title": "Functional Characteristics",
		  "$ref": "./AssetFC.json"
		}
	}
}
