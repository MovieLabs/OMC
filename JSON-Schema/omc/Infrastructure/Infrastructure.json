{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "",
	"type": "object",
	"description": "The underlying systems and framework required for the production of the Creative Work; it is generally not specific to a particular Creative Work.",
	"title": "Infrastructure",
	"required": [
		"identifier",
		"entityType"
	],
	"properties": {
		"entityInfo": {
			"title": "Entity Information",
			"$ref": "../Model/definitions.json#/properties/entityInfo"
		},
		"entityType": {
			"type": "string",
			"title": "Entity Type",
			"const": "Infrastructure"
		},
		"identifier": {
			"$ref": "../Utility/identifier.json"
		},
		"name": {
			"type": [
				"string",
				"null"
			],
			"title": "Infrastructure name",
			"description": "A human readable name for the infrastructure"
		},
		"description": {
			"type": [
				"string",
				"null"
			],
			"title": "Infrastructure description",
			"description": "A human readable description of the infrastructure"
		},
		"customData": {
			"title": "Custom Data",
			"$ref": "../Model/definitions.json#/properties/customData"
		},
		"structuralCharacteristics": {
			"title": "Participant Structural Characteristics",
			"properties": {
				"structuralType": {
					"title": "Structural Type",
					"type": "string"
				}
			}
		},
		"functionalCharacteristics": {
			"title": "Participant Functional Characteristics",
			"properties": {
				"functionalType": {
					"title": "Functional Type",
					"type": "string"
				}
			}
		},
		"Context": {
			"title": "Asset Context",
			"$ref": "../MediaCreationContext/Context.json"
		}
	}
}
