{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "",
  "type": "object",
  "title": "Narrative Effect",
  "description": "An on screen visual effect considered essential to the narrative",
  "required": [
    "entityType",
    "identifier"
  ],
  "properties": {
    "entityInfo": {
      "title": "Entity Information",
      "$ref": "../Model/definitions.json#/properties/entityInfo"
    },
    "entityType": {
      "title": "Entity Type",
      "type": "string",
      "const": "NarrativeEffect"
    },
    "identifier": {
      "$ref": "../Utility/identifier.json"
    },
    "name": {
      "title": "Narrative Effect Name",
      "description": "A human readable name for the effect",
      "type": [
        "string",
        "null"
      ]
    },
    "description": {
      "title": "Narrative Object Description",
      "description": "A brief description of the narrative effect",
      "type": [
        "string",
        "null"
      ]
    },
    "objectType": {
      "title": "Object type",
      "description": "A categorization of the type effect.",
      "enum": [
        "effect",
        "sfx",
        "vfx"
      ]
    },
    "customData": {
      "title": "Custom Data",
      "$ref": "../Model/definitions.json#/properties/customData"
    },
    "Context": {
      "title": "Narrative Effect Context",
      "$ref": "./Context.json"
    }
  }
}
