{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "",
	"title": "Editorial Sequence",
	"description": "A sequence of shots linked to creative intent.",
	"type": "object",
	"required": [
		"entityType",
		"identifier"
	],
	"properties": {
		"entityInfo": {
			"title": "Entity Information",
			"$ref": "../Model/definitions.json#/properties/entityInfo"
		},
		"entityType": {
			"title": "Entity Type",
			"type": "string",
			"const": "Sequence"
		},
		"identifier": {
			"$ref": "../Utility/identifier.json"
		},
		"name": {
			"title": "Sequence Name",
			"description": "A human readable name for the editorial sequence",
			"type": [
				"string",
				"null"
			]
		},
		"description": {
			"title": "Editorial Sequence Description",
			"description": "A brief description of the editorial sequence",
			"type": [
				"string",
				"null"
			]
		},
		"sequenceType": {
			"description": "The specific type of sequence",
			"enum": ["editorial", "image", "animation", "vfx", "color"]
		},
		"sequenceChronologyDescriptor": {
			"type": "array",
			"items": {
				"properties": {
					"order": {
						"type": "number"
					},
					"source": {
						"$ref": "../Asset/Asset.json"
					}
				}
			}
		},
		"customData": {
			"title": "Custom Data",
			"$ref": "../Model/definitions.json#/properties/customData"
		},
		"Context": {
			"title": "Editorial Sequence Context",
			"$ref": "./Context.json"
		}
	}
}