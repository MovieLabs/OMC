{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "",
  "type": "object",
  "title": "Narrative Audio",
  "description": "A named piece of audio significant to the narrative.",
  "required": [
    "entityType",
    "identifier"
  ],
  "properties": {
    "entityInfo": {
      "title": "Entity Information",
      "$ref": "../Model/definitions.json#/properties/entityInfo"
    },
    "entityType": {
      "title": "Entity Type",
      "type": "string",
      "const": "NarrativeAudio"
    },
    "identifier": {
      "$ref": "../Utility/identifier.json"
    },
    "name": {
      "title": "Narrative Object Name",
      "description": "A human readable name for the audio clip",
      "type": [
        "string",
        "null"
      ]
    },
    "description": {
      "title": "Narrative Audio Description",
      "description": "A brief description of the narrative audio",
      "type": [
        "string",
        "null"
      ]
    },
    "objectType": {
      "title": "Object type",
      "description": "A categorization of the type of audio.",
      "enum": [
        "audio",
        "soundEffect",
        "music"
      ]
    },
    "customData": {
      "title": "Custom Data",
      "$ref": "../Model/definitions.json#/properties/customData"
    },
    "Context": {
      "title": "Narrative Audio Context",
      "$ref": "./Context.json"
    }
  }
