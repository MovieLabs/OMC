{	"$schema": "http://json-schema.org/draft-07/schema#",	"title": "Participant",	"description": "The entities (people, organizations, and services) that are responsible for the production of the Creative Work.",	"type": "object",	"required": [		"entityType",		"identifier"	],	"properties": {		"entityInfo": {			"title": "Entity Information",			"$ref": "../Model/definitions.json#/properties/entityInfo"		},		"entityType": {			"title": "Entity Type",			"type": "string",			"const": "Participant"		},		"identifier": {			"$ref": "../Utility/identifier.json"		},		"description": {			"title": "Description",			"description": "A brief description of the participant",			"type": [				"string",				"null"			]		},		"ParticipantGroup": {			"title": "Participant Group",			"$ref": "./ParticipantGroup.json"		},		"roleGroup": {			"description": "A group of Roles this Participant can perform",			"type": "array",			"items": {				"$ref": "../Participant/Role.json"			}		},		"StructuralCharacteristics": {			"title": "Participant Structural Characteristics",			"$ref": "../Participant/ParticipantSC.json"		},		"FunctionalCharacteristics": {			"title": "Participant Functional Characteristics",			"$ref": "../Participant/ParticipantFC.json"		}	}}