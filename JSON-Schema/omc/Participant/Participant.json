{  "$schema": "http://json-schema.org/draft-07/schema#",  "$id": "",  "title": "Participant",  "description": "The entities (people, organizations, and services) that are responsible for the production of the Creative Work.",  "type": "object",  "required": [    "entityType",    "identifier"  ],  "properties": {    "entityInfo": {      "title": "Entity Information",      "$ref": "../Model/definitions.json#/properties/entityInfo"    },    "entityType": {      "title": "Entity Type",      "type": "string",      "const": "Participant"    },    "identifier": {      "$ref": "../Utility/identifier.json"    },    "name": {      "type": [        "string",        "null"      ],      "title": "Participant name",      "description": "A human readable name for the participant, primarily for use as a label"    },    "description": {      "title": "Description",      "description": "A brief description of the participant",      "type": [        "string",        "null"      ]    },    "Participant": {      "title": "Participant Group",      "type": "array",      "items": {        "$ref": "./Participant.json"      }    },    "structuralCharacteristics": {      "title": "Participant Structural Characteristics",      "properties": {        "structuralType": {          "title": "Structural Type",          "type": "string"        },        "Person": {          "$ref": "./Person.json"        },        "Organization": {          "$ref": "./Organization.json"        }      }    },    "functionalCharacteristics": {      "title": "Participant Functional Characteristics",      "properties": {        "functionalType": {          "title": "Functional Type",          "type": "string"        },        "Role": {          "description": "A group of Roles this Participant can perform",          "type": "array",          "items": {            "$ref": "../Participant/Role.json"          }        }      }    },    "Context": {      "title": "Character Context",      "$ref": "../MediaCreationContext/Context.json"    }  }}