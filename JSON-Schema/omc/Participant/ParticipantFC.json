{	"$schema": "http://json-schema.org/draft-07/schema#",	"$id": "",	"type": "object",	"title": "Participant Functional Characteristics",	"description": "A set of Functional Characteristics for Participants",	"required": [		"identifier",		"entityType"	],	"properties": {		"entityInfo": {			"title": "Entity Information",			"$ref": "../Model/definitions.json#/properties/entityInfo"		},		"entityType": {			"type": "string",			"title": "Entity Type",			"const": "ParticipantFC"		},		"identifier": {			"$ref": "../Utility/identifier.json"		},		"functionalType": {			"type": "string"		},		"functionalProperties": {			"$ref": "#/definitions/functionalProperties"		}	},	"definitions": {		"functionalProperties": {			"type": "object",			"properties": {				"name": {					"type": "string",					"title": "Name",					"description": "A name used to describe what this Participant does, often a job title"				},				"description": {					"type": "string",					"title": "Description",					"description": "A brisf description of what this Participant does on the production"				},				"Role": {					"type": "array",					"items": {						"$ref": "./Role.json"					}				}			}		}	}}