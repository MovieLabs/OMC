{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "",
	"type": "object",
	"title": "Participant Structural Characteristics",
	"description": "A set of Structural Characteristics for Participants",
	"required": [
		"identifier",
		"entityType"
	],
	"properties": {
		"entityInfo": {
			"title": "Entity Information",
			"$ref": "../Model/definitions.json#/properties/entityInfo"
		},
		"entityType": {
			"type": "string",
			"title": "Entity Type",
			"const": "ParticipantSC"
		},
		"identifier": {
			"$ref": "../Utility/identifier.json"
		},
		"structuralType": {
			"type": "string"
		},
		"structuralProperties": {
			"$ref": "#/definitions/structuralProperties"
		}
	},
	"definitions": {
		"structuralProperties": {
			"anyOf": [
				{
					"title": "Organization",
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"participantName": {
							"type": "object",
							"title": "Organization Name",
							"properties": {
								"fullName": {
									"type": "string",
									"title": "Organization full name",
									"description": "Organizations full name"
								},
								"alternateName": {
									"type": "string",
									"title": "Organization alternate name",
									"description": "An alternate or shortend name"
								}
							}
						},
						"Location": {
							"$ref": "../Utility/Location.json"
						},
						"contact": {
							"$ref": "../Utility/contact.json"
						}
					}
				},
				{
					"type": "object",
					"title": "Department",
					"additionalProperties": false,
					"properties": {
						"participantName": {
							"type": "object",
							"title": "Department Name",
							"properties": {
								"fullName": {
									"type": "string",
									"title": "Department full name",
									"description": "Department's full name"
								},
								"alternateName": {
									"type": "string",
									"title": "Department alternate name",
									"description": "An alternate or shortend name"
								}
							}
						},
						"Location": {
							"$ref": "../Utility/Location.json"
						},
						"contact": {
							"$ref": "../Utility/contact.json"
						}
					}
				},
				{
					"type": "object",
					"title": "Person",
					"additionalProperties": false,
					"properties": {
						"participantName": {
							"$ref": "../Utility/completeName.json"
						},
						"jobTitle": {
							"type": [
								"string",
								"null"
							],
							"title": "Job Title",
							"description": "A persons job title (as distinct from a specific role)"
						},
						"gender": {
							"$ref": "../Utility/gender.json"
						},
						"Location": {
							"$ref": "../Utility/Location.json"
						},
						"contact": {
							"$ref": "../Utility/contact.json"
						}
					}
				},
				{
					"type": "object",
					"title": "Service",
					"additionalProperties": false,
					"properties": {
						"participantName": {
							"type": "object",
							"title": "Service Name",
							"properties": {
								"fullName": {
									"type": "string",
									"title": "Service full name",
									"description": "Service's full name"
								},
								"alternateName": {
									"type": "string",
									"title": "Service alternate name",
									"description": "An alternate or shortend name"
								}
							}
						},
						"Location": {
							"$ref": "../Utility/Location.json"
						},
						"contact": {
							"$ref": "../Utility/contact.json"
						}
					}
				}
			]
		}
	}
}