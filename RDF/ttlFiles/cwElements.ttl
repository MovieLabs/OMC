# baseURI: http://mcw.movielabs.com/ontology/cwElements
# imports: http://mcw.movielabs.com/ontology

@prefix mcw: <http://mcw.movielabs.com/ontology#> .
@prefix mmc: <http://mmc.movielabs.com/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

mcw:Artwork
  rdf:type owl:Class ;
  rdfs:label "Artwork" ;
  rdfs:subClassOf mmc:AnythingWithACountry ;
  rdfs:subClassOf mmc:Attributable ;
.
mcw:hasArtwork
  rdf:type owl:ObjectProperty ;
  rdfs:domain mcw:CreativeWork ;
  rdfs:label "has artwork" ;
  rdfs:range mcw:Artwork ;
.
mcw:hasArtworkHeight
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mcw:Artwork ;
  rdfs:label "has artwork height" ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          xsd:decimal
          xsd:integer
        ) ;
    ] ;
.
mcw:hasArtworkType
  rdf:type owl:DatatypeProperty ;
  rdfs:domain <http://www.movielabs.org/creativeworks/model#Artwork> ;
  rdfs:label "has artwork type" ;
  rdfs:range xsd:string ;
  skos:editorialNote "In the future we may create an enumeration class of artwork types, but as it is not yet fully worked out, we keep it informal." ;
.
mcw:hasArtworkWidth
  rdf:type owl:DatatypeProperty ;
  rdfs:domain mcw:Artwork ;
  rdfs:label "has artwork width" ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          xsd:decimal
          xsd:integer
        ) ;
    ] ;
.
<http://mcw.movielabs.com/ontology/cwElements>
  rdf:type owl:Ontology ;
  owl:imports <http://mcw.movielabs.com/ontology> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
