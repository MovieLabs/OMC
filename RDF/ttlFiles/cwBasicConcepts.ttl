@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

mcw:Award
  rdf:type owl:Class ;
  rdfs:label "Award" ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#AnythingWithAnID> ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#Attributable> ;
.
mcw:AwardCategory
  rdf:type owl:Class ;
  rdfs:label "Award category" ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#VocabularyTerm> ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "The subject of an award, e.g., best film, best screenplay, best song, best supporting actor ..." ;
.
mcw:AwardGrantor
  rdf:type owl:Class ;
  rdfs:label "Award grantor" ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#VocabularyTerm> ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "The organization granting an award, e.g., BAFTA, AMPAS ..." ;
.
mcw:Awardable
  rdf:type owl:Class ;
  rdfs:label "Awardable" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Anything capable of receiving an award." ;
.
mcw:AwardingBody
  rdf:type owl:Class ;
  rdfs:label "AwardingBody" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Any organization that can issue an award." ;
.
mcw:BasedOnThis
  rdf:type owl:Class ;
  rdfs:label "Based on this" ;
  rdfs:subClassOf mcw:RelatedThing ;
  skos:definition "Thing on which a Creative Work is based. BasedOnThis is not a subclass of RelatedWork, because the latter is a Creative Work, while BasedOnThis points to, for example, the novel a film is based on." ;
.
mcw:CreativeWork
  rdf:type owl:Class ;
  rdfs:label "Creative work" ;
  rdfs:subClassOf mcw:Awardable ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#AnythingWithAnID> ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#Attributable> ;
  skos:definition "Within this ontology, the notion of a CreativeWork is limited to a movie, video, TV program, or similar \"playable\" media. It does not include books, plays, etc." ;
.
mcw:Edit
  rdf:type owl:Class ;
  rdfs:label "Edit" ;
  rdfs:subClassOf owl:Thing ;
.
mcw:RelatedThing
  rdf:type owl:Class ;
  rdfs:label "Related thing" ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#AnythingWithAnID> ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#Attributable> ;
  skos:definition "This is an umbrella concept to group the similar, but different, concepts of RelatedWork and BasedOnThis. A RelatedWork is a CreativeWork to which some other CreativeWork stands in some relation. A BasedOnThis is an entity that served as the basis for a CreativeWork, e.g., a novel that is the basis for a movie. Even though such entities are creative works in a more general sense, within this ontology the notion of a CreativeWork is limited to a movie, video, TV program, or similar \"playable\" media." ;
.
mcw:RelatedWork
  rdf:type owl:Class ;
  rdfs:label "Related work" ;
  rdfs:subClassOf mcw:CreativeWork ;
  rdfs:subClassOf mcw:RelatedThing ;
  skos:definition "A CreativeWork to which another CreativeWork is related" ;
  skos:editorialNote "This is a generic expression of the existence of some kind of relationship between two 'creative' things, i.e. not between a creative work and company or a person.  If we're lucky, there will be an identifier for the related item, via mmc:hasIdentifier. " ;
.
mcw:Title
  rdf:type owl:Class ;
  rdfs:label "Title" ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#AnythingWithACountry> ;
  rdfs:subClassOf <http://mmc.movielabs.com/ontology#Attributable> ;
.
<http://mcw.movielabs.com/ontology>
  rdf:type owl:Ontology ;
  owl:imports <http://mmc.movielabs.com/ontology> ;
