mmd:Percentage
  rdf:type owl:Class ;
  rdfs:label "Percentage" ;
  rdfs:subClassOf mmd:BracketedValue ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:int ;
      owl:onProperty mmd:hasNumericValue ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue 0 ;
      owl:onProperty mmd:hasLowScale ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue 100 ;
      owl:onProperty mmd:hasHighScale ;
    ] ;
  skos:definition "Measures the degree of something" ;
