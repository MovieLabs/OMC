  rdfs:subClassOf mmco:Portrayer ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          mmco:Portrayal
          [
            rdf:type owl:Restriction ;
            owl:allValuesFrom mmco:Person ;
            owl:onProperty mmco:hasPortrayer ;
